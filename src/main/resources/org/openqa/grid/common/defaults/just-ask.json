              "image": "selenium/standalone-chrome:3.4.0-chromium",
              "port": "4444",
              "volumes": [
                "/dev/shm:/dev/shm"
              ],
              "env": [
                "JAVA_OPTS=-Xmx512m",
                "TZ=UTC",
                "SCREEN_WIDTH=1360",

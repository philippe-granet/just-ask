{
	"dockerRestApiUri": "http://192.168.43.130:2375",
	"localhost": "0.0.0.0",
	"maxSession": 5,
	"browsers": [
		{
			"browser": "internet explorer",
			"defaultVersion": "11",
			"versions": [
				{
					"version": "11",
					"implementation": "com.rationaleemotions.server.JvmBasedSeleniumServer",
					"target": {
					}
				}
			]
		},
		{
			"browser": "chrome",
			"defaultVersion": "60.0",
			"versions": [
				{
					"version": "60.0",
					"implementation": "com.rationaleemotions.server.DockerBasedSeleniumServer",
					"target": {
						"image": "selenium/standalone-chrome:3.4.0",
						"port": "4444",
						"volumes": [
							"/dev/shm:/dev/shm"
						],
						"env": [
							"JAVA_OPTS=-Xmx512m",
							"TZ=Europe/Paris"
						]
					}
				}
			]
		},
		{
			"browser": "firefox",
			"defaultVersion": "54.0",
			"versions": [
				{
					"version": "54.0",
					"implementation": "com.rationaleemotions.server.DockerBasedSeleniumServer",
					"target": {
						"image": "selenium/standalone-firefox:3.4.0",
						"port": "4444",
						"shmSize": "2g",
						"env": [
							"JAVA_OPTS=-Xmx512m",
							"TZ=Europe/Paris"
						]
					}
				}
			]
		}
	]

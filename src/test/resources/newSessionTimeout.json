{
  "host": null,
  "port": 1234,
  "role": "hub",
  "maxSession": 1,
  "newSessionWaitTimeout": 10000,
  "capabilityMatcher": "org.openqa.grid.internal.utils.DefaultCapabilityMatcher",
  "throwOnCapabilityNotPresent": true,
  "jettyMaxThreads": -1,
  "cleanUpCycle": 1000,
  "browserTimeout": 0,
  "timeout": 300000,
  "debug": false,
  "servlets": [
    "com.rationaleemotions.servlets.JustAskServlet",
    "com.rationaleemotions.servlets.HubRequestsProxyingServlet"
  ],
  "withoutServlets": [
  ],
  "justAsk": {
    "dockerRestApiUri": "unix:///var/run/docker.sock",
    "localhost": "0.0.0.0",
    "maxSession": 1,
    "browsers": [
      {
        "browser": "chrome",
        "defaultVersion": "64.0.3282.140",
        "versions": [
          {
            "version": "64.0.3282.140",
            "implementation": "com.rationaleemotions.server.DockerBasedSeleniumServer",
            "target": {
              "image": "selenium/standalone-chrome:3.9.1-actinium",
              "port": "4444",
              "volumes": [
                "/dev/shm:/dev/shm"
              ],
              "env": [
                "JAVA_OPTS=-Xmx512m",
                "TZ=UTC",
                "SCREEN_WIDTH=1360",
                "SCREEN_HEIGHT=1020",
                "SCREEN_DEPTH=24"
              ]
            }
          }
        ]
      },
      {
        "browser": "firefox",
        "defaultVersion": "58.0.1",
        "versions": [
          {
            "version": "58.0.1",
            "implementation": "com.rationaleemotions.server.DockerBasedSeleniumServer",
            "target": {
              "image": "selenium/standalone-firefox:3.9.1-actinium",
              "port": "4444",
              "volumes": [
                "/dev/shm:/dev/shm"
              ],
              "shmSize": "2g",
              "env": [
                "JAVA_OPTS=-Xmx512m",
                "TZ=UTC",
                "SCREEN_WIDTH=1360",
                "SCREEN_HEIGHT=1020",
                "SCREEN_DEPTH=24"
              ]
            }
          }
        ]
      }
    ]
  }
}
